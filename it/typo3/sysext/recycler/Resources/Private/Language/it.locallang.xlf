      </trans-unit>
      <trans-unit id="table.header.owner" approved="yes">
        <source>Owner</source>
        <target state="translated">Proprietario</target>
      </trans-unit>
      <trans-unit id="table.header.path" approved="yes">
        <source>Path</source>
        <target state="translated">Percorso</target>
      </trans-unit>
    </body>
