<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns:t3="http://typo3.org/schemas/xliff" version="1.0">
  <file t3:id="1415814881" source-language="en" datatype="plaintext" original="messages" date="2011-10-17T20:22:34Z" product-name="lang" target-language="el">
    <header/>
    <body>
      <trans-unit id="mlang_labels_tablabel" approved="yes">
        <source>List of database-records</source>
        <target state="translated">Λίστα των εγγραφών της βάσης δεδομένων</target>
      </trans-unit>
      <trans-unit id="mlang_labels_tabdescr" approved="yes">
        <source>The Web&gt;List module provides low level access to records appearing on a page.</source>
        <target state="translated">Η μονάδα Ιστός&gt;Λίστα δίνει τη δυνατότητα χαμηλού επιπέδου πρόσβασης στις εγγραφές που εμφανίζονται σε μία σελίδα</target>
      </trans-unit>
      <trans-unit id="mlang_tabs_tab" approved="yes">
        <source>List</source>
        <target state="translated">Λίστα</target>
      </trans-unit>
      <trans-unit id="clip_pasteAfter" approved="yes">
        <source>Paste after: Clipboard content is inserted after this record</source>
        <target state="translated">Επικόληση μετά: Το περιεχόμενο του πρόχειρου εισάγεται μετά από αυτή την εγγραφή</target>
      </trans-unit>
      <trans-unit id="clip_pasteInto" approved="yes">
        <source>Paste into: Clipboard content is inserted on this page</source>
        <target state="translated">Επικόληση εντός: Το περιεχόμενο του πρόχειρου εισάγετε μέσα σε αυτή τη σελίδα</target>
      </trans-unit>
      <trans-unit id="clip_paste" approved="yes">
        <source>Paste in clipboard content</source>
        <target state="translated">Επικόληση του περιεχομένου του πρόχειρου</target>
      </trans-unit>
      <trans-unit id="clip_markRecords" approved="yes">
        <source>Mark All/Mark none</source>
        <target state="translated">Σημείωση Όλων/Σημείωση κανενός</target>
      </trans-unit>
      <trans-unit id="clip_selectMarked" approved="yes">
        <source>Transfer the selection of records to clipboard</source>
        <target state="translated">Μεταφορά των επιλεγμένων εγγραφών στο πρόχειρο</target>
      </trans-unit>
      <trans-unit id="clip_duplicates" approved="yes">
        <source>Mark duplicates</source>
        <target state="translated">Σημείωση διπλότυπων</target>
      </trans-unit>
      <trans-unit id="clip_editMarked" approved="yes">
        <source>Edit marked</source>
        <target state="translated">Επεξεργασία σημειωμένων</target>
      </trans-unit>
      <trans-unit id="clip_deleteMarked" approved="yes">
        <source>Delete marked</source>
        <target state="translated">Διαγραφή σημειωμένων</target>
      </trans-unit>
      <trans-unit id="clip_deleteMarkedWarning" approved="yes">
        <source>Are you sure you want to delete all marked records from the table '%s'?</source>
        <target state="translated">Είστε σίγουρος ότι θέλετε να διαγράψετε όλες τις σημειωμένες εγγραφές από τον πίνακα '%s';</target>
      </trans-unit>
      <trans-unit id="showInfo" approved="yes">
        <source>Display information</source>
        <target state="translated">Εμφάνιση πληροφοριών</target>
      </trans-unit>
      <trans-unit id="edit" approved="yes">
        <source>Edit record</source>
        <target state="translated">Επεξεργασία εγγραφής</target>
      </trans-unit>
      <trans-unit id="editPage" approved="yes">
        <source>Edit page properties</source>
        <target state="translated">Επεξεργασία ιδιοτήτων σελίδας</target>
      </trans-unit>
      <trans-unit id="new" approved="yes">
        <source>New record</source>
        <target state="translated">Νέα εγγραφή</target>
      </trans-unit>
      <trans-unit id="newPage" approved="yes">
        <source>Create new page after this page</source>
        <target state="translated">Δημιουργία νέας σελιδας μετά από αυτή τη σελίδα</target>
      </trans-unit>
      <trans-unit id="newRecord" approved="yes">
        <source>Create new record after this record</source>
        <target state="translated">Δημιουργία νέας εγγραφής μετά από αυτή την εγγραφή</target>
      </trans-unit>
      <trans-unit id="newRecordGeneral" approved="yes">
        <source>Create new record</source>
        <target state="translated">Δημιουργία Νέας εγγραφής</target>
      </trans-unit>
      <trans-unit id="moveUp" approved="yes">
        <source>Move up in list</source>
        <target state="translated">Μεταφορά προς τα επάνω στη λίστα</target>
      </trans-unit>
      <trans-unit id="moveDown" approved="yes">
        <source>Move down in list</source>
        <target state="translated">Μεταφορά προς τα κάτω στη λίστα</target>
      </trans-unit>
      <trans-unit id="move_record" approved="yes">
        <source>Re-position content element</source>
        <target state="translated">Αλλαγή θέσης στοιχείου περιεχομένου</target>
      </trans-unit>
      <trans-unit id="move_page" approved="yes">
        <source>Move page</source>
        <target state="translated">Μεταφροά σελίδας</target>
      </trans-unit>
      <trans-unit id="hide" approved="yes">
        <source>Hide record</source>
        <target state="translated">Απόκρυψη εγγραφής</target>
      </trans-unit>
      <trans-unit id="unHide" approved="yes">
        <source>Un-hide record</source>
        <target state="translated">Μη-απόκρυψη εγγραφής</target>
      </trans-unit>
      <trans-unit id="hidePage" approved="yes">
        <source>Hide page</source>
        <target state="translated">Απόκρυψη σελίδας</target>
      </trans-unit>
      <trans-unit id="unHidePage" approved="yes">
        <source>Un-hide page</source>
        <target state="translated">Μη-απόκρυψη σελίδας</target>
      </trans-unit>
      <trans-unit id="prevLevel" approved="yes">
        <source>Move this page to the position after the current parent page (Inwards)</source>
        <target state="translated">Μετακίνηση αυτής της σελίδας στην θέση μετά την τρέxουχα σελίδα-γονέα (Προς τα μέσα)</target>
      </trans-unit>
      <trans-unit id="nextLevel" approved="yes">
        <source>Move this page to be a subpage of the page above (Outwards)</source>
        <target state="translated">Μετακίνηση αυτής της σελίδας έτσι ώστε να είναι υποσελίδα της παραπάνω σελίδας (Προς τα έξω)</target>
      </trans-unit>
      <trans-unit id="delete" approved="yes">
        <source>Delete record (!)</source>
        <target state="translated">Διαγραφή εγγραφής (!)</target>
      </trans-unit>
      <trans-unit id="history" approved="yes">
        <source>Display change history / Un-do</source>
        <target state="translated">Εμφάνιση ιστορικού αλλαγών / Αναίρεση</target>
      </trans-unit>
      <trans-unit id="displayVersions" approved="yes">
        <source>Display versions</source>
        <target state="translated">Εμφάνιση εκδόσεων</target>
      </trans-unit>
      <trans-unit id="permissions" approved="yes">
        <source>Set permissions for page</source>
        <target state="translated">Καθορισμός αδειών για την σελίδα</target>
      </trans-unit>
      <trans-unit id="largeControl" approved="yes">
        <source>Extended view</source>
        <target state="translated">Εκτεταμένη προβολή</target>
      </trans-unit>
      <trans-unit id="contractView" approved="yes">
        <source>List all tables</source>
        <target state="translated">Εμφάνιση περιεχομένων όλων των πινάκων</target>
      </trans-unit>
      <trans-unit id="expandView" approved="yes">
        <source>List only this table</source>
        <target state="translated">Εμφάνιση περιεχομένων μόνο αυτού του πίνακα</target>
      </trans-unit>
      <trans-unit id="showClipBoard" approved="yes">
        <source>Show clipboard</source>
        <target state="translated">Εμφάνιση πρόχειρου</target>
      </trans-unit>
      <trans-unit id="editShownColumns" approved="yes">
        <source>Edit all shown fields of the listed records</source>
        <target state="translated">Επεξεργασία όλων των εμφανιζόμενων πεδίων των εγγραφών που εμφανίζονται</target>
      </trans-unit>
      <trans-unit id="editThisColumn" approved="yes">
        <source>Edit the '%s' field of the listed records</source>
        <target state="translated">Επεξεργασία του '%s' πεδίου των εγγραφών που εμφανίζονται</target>
      </trans-unit>
      <trans-unit id="deleteWarning" approved="yes">
        <source>Are you sure you want to delete this record?</source>
        <target state="translated">Είστε σίγουρος ότι θέλετε να διαγράψετε αυτή την εγγραφή;</target>
      </trans-unit>
      <trans-unit id="localization" approved="yes">
        <source>Localization view</source>
        <target state="translated">Προβολή τοπικοποίησης</target>
      </trans-unit>
      <trans-unit id="defaultLanguage" approved="yes">
        <source>Default</source>
        <target state="translated">Προεπιλεγμένο</target>
      </trans-unit>
      <trans-unit id="multipleLanguages" approved="yes">
        <source>All languages</source>
        <target state="translated">Όλες οι γλώσσες</target>
      </trans-unit>
      <trans-unit id="viewRecords" approved="yes">
        <source>Viewing records of</source>
        <target state="translated">Εμφανίζονται εγγραφές από</target>
      </trans-unit>
    </body>
