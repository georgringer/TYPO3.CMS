        <source>Abstracts</source>
        <target state="translated">Abstracts</target>
      </trans-unit>
      <trans-unit id="menu_abstract.description">
        <source>Menu of subpages of selected pages including abstracts</source>
        <target state="translated">Menü der Unterseiten der ausgewählten Seiten inklusive Abstracts</target>
      </trans-unit>
      <trans-unit id="menu_categorized_content.title">
        <source>Categorized content</source>
        <target state="translated">Kategorisierter Inhalt</target>
      </trans-unit>
      <trans-unit id="menu_categorized_content.description">
        <source>Content elements for selected categories</source>
        <target state="translated">Inhaltselemente für ausgewählte Kategorien</target>
      </trans-unit>
      <trans-unit id="menu_categorized_pages.title">
        <source>Categorized pages</source>
        <target state="translated">Kategorisierte Seiten</target>
      </trans-unit>
      <trans-unit id="menu_pages.title">
        <source>Pages</source>
        <target state="translated">Seiten</target>
      </trans-unit>
      <trans-unit id="menu_pages.description">
        <source>Menu of selected pages</source>
        <target state="translated">Menü der ausgewählten Seiten</target>
      </trans-unit>
      <trans-unit id="menu_subpages.title">
        <source>Subpages</source>
        <target state="translated">Unterseiten</target>
      </trans-unit>
      <trans-unit id="menu_subpages.description">
        <source>Menu of subpages of selected pages</source>
        <target state="translated">Menü der Unterseiten der ausgewählten Seiten</target>
      </trans-unit>
      <trans-unit id="menu_categorized_pages.description">
        <source>Pages for selected categories</source>
        <target state="translated">Seiten für ausgewählte Kategorien</target>
      </trans-unit>
      <trans-unit id="menu_recently_updated.title">
        <source>Recently updated pages</source>
        <target state="translated">Kürzlich aktualisierte Seiten</target>
      </trans-unit>
      <trans-unit id="menu_recently_updated.description">
        <source>Menu of recently updated pages</source>
        <target state="translated">Menü der kürzlich aktualisierten Seiten</target>
      </trans-unit>
      <trans-unit id="menu_related_pages.title">
        <source>Related pages</source>
        <target state="translated">Verwandte Seiten</target>
      </trans-unit>
      <trans-unit id="menu_related_pages.description">
        <source>Menu of related pages based on keywords</source>
        <target state="translated">Menü der verwandten Seiten basierend auf Schlüsselwörtern</target>
      </trans-unit>
      <trans-unit id="menu_section.title">
        <source>Section index</source>
        <target state="translated">Sektionsindex</target>
      </trans-unit>
      <trans-unit id="menu_section.description">
        <source>Page content marked for section menus</source>
        <target state="translated">Seiteninhalt für Sektionsmenüs markiert</target>
      </trans-unit>
      <trans-unit id="menu_section_pages.title">
        <source>Section index of subpages from selected pages</source>
        <target state="translated">Abschnitt-Index der Unterseiten von ausgewählten Seiten</target>
      </trans-unit>
      <trans-unit id="menu_section_pages.description">
        <source>Menu of subpages of selected pages including sections</source>
        <target state="translated">Menü der Unterseiten der ausgewählten Seiten inklusive Abschnitte</target>
      </trans-unit>
      <trans-unit id="menu_sitemap.title">
        <source>Sitemap</source>
        <target state="translated">Sitemap</target>
      </trans-unit>
      <trans-unit id="menu_sitemap.description">
        <source>Expanded menu of all subpages of the current site</source>
        <target state="translated">Erweitertes Menü aller Unterseiten der aktuellen Seite</target>
      </trans-unit>
      <trans-unit id="menu_sitemap_pages.title">
        <source>Sitemaps of selected pages</source>
        <target state="translated">Sitemaps der ausgewählten Seiten</target>
      </trans-unit>
      <trans-unit id="menu_sitemap_pages.description">
        <source>Expanded menu of all subpages for selected pages</source>
        <target state="translated">Erweitertes Menü aller Unterseiten für ausgewählte Seiten</target>
      </trans-unit>
      <trans-unit id="special">
        <source>Special elements</source>
        <target state="translated">Spezielle Elemente</target>
      </trans-unit>
      <trans-unit id="special_filelinks_title">
        <source>File Links</source>
        <target state="translated">Dateilinks</target>
      </trans-unit>
      <trans-unit id="special_filelinks_description">
        <source>Makes a list of files for download.</source>
        <target state="translated">Erstellt eine Liste der Dateien zum Download.</target>
      </trans-unit>
      <trans-unit id="special_sitemap_title">
        <source>Sitemap</source>
        <target state="translated">Sitemap</target>
      </trans-unit>
      <trans-unit id="special_menus_title">
        <source>Special Menus</source>
        <target state="translated">Spezielle Menüs</target>
      </trans-unit>
      <trans-unit id="special_menus_description">
        <source>Creates a menu of pages, sitemap or other special menus.</source>
        <target state="translated">Erzeugt ein Menü von Seiten, Sitemap oder anderen speziellen Menüs.</target>
      </trans-unit>
      <trans-unit id="special_plainHTML_title">
        <source>Plain HTML</source>
        <target state="translated">Einfaches HTML</target>
      </trans-unit>
      <trans-unit id="special_plainHTML_description">
        <source>With this element you can insert raw HTML code on the page.</source>
        <target state="translated">Mit diesem Element können Sie RohHTML-Code auf der Seite einfügen.</target>
      </trans-unit>
      <trans-unit id="special_divider_title">
        <source>Divider</source>
        <target state="translated">Taucher</target>
      </trans-unit>
      <trans-unit id="special_divider_description">
        <source>This element inserts a visual divider, which is by default a horizontal line.</source>
        <target state="translated">Dieses Element fügt einen visuellen Trennzeichen ein, der standardmäßig eine horizontale Linie ist.</target>
      </trans-unit>
      <trans-unit id="special_shortcut_title">
        <source>Insert records</source>
        <target state="translated">Datensätze einfügen</target>
      </trans-unit>
      <trans-unit id="special_shortcut_description">
        <source>With this element you can embed other content elements.</source>
        <target state="translated">Mit diesem Element können Sie andere Inhaltselemente einbinden.</target>
      </trans-unit>
      <trans-unit id="forms">
        <source>Form elements</source>
        <target state="translated">Formularelemente</target>
      </trans-unit>
      <trans-unit id="forms_mail_title">
        <source>Mail Form</source>
        <target state="translated">Mail-Formular</target>
      </trans-unit>
      <trans-unit id="forms_mail_description">
