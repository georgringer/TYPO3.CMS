    <body>
      <trans-unit id="sel1">
        <source>Please select the type of page content you wish to create</source>
        <target state="translated">Bitte wählen Sie den Typ der Seiteninhalte aus, die Sie erstellen möchten</target>
      </trans-unit>
      <trans-unit id="sel2">
        <source>Now, click the position where you wish to place the element on the page</source>
        <target state="translated">Klicken Sie nun auf die Position, in der Sie das Element auf der Seite platzieren möchten</target>
      </trans-unit>
      <trans-unit id="common">
        <source>Typical page content</source>
        <target state="translated">Typischer Seiteninhalt</target>
      </trans-unit>
      <trans-unit id="common_headerOnly_title">
        <source>Header Only</source>
        <target state="translated">Nur Header</target>
      </trans-unit>
      <trans-unit id="common_headerOnly_description">
        <source>Adds a header only.</source>
        <target state="translated">Fügt nur einen Header hinzu.</target>
      </trans-unit>
      <trans-unit id="common_regularText_title">
        <source>Regular Text Element</source>
        <target state="translated">Regelmäßiges Textelement</target>
      </trans-unit>
      <trans-unit id="common_regularText_description">
        <source>A regular text element with header and bodytext fields.</source>
        <target state="translated">Ein reguläres Textelement mit Kopfzeilen- und Body-Feldern.</target>
      </trans-unit>
      <trans-unit id="common_textImage_title">
        <source>Text &amp; Images</source>
        <target state="translated">Text &amp; Bilder</target>
      </trans-unit>
      <trans-unit id="common_textImage_description">
        <source>Any number of images wrapped right around a regular text element.</source>
        <target state="translated">Jede Anzahl von Bildern, die direkt um ein reguläres Textelement umhüllt wurden.</target>
      </trans-unit>
      <trans-unit id="common_textMedia_title">
        <source>Text &amp; Media</source>
        <target state="translated">Text &amp; Medien</target>
      </trans-unit>
      <trans-unit id="common_textMedia_description">
        <source>Any number of media wrapped right around a regular text element.</source>
        <target state="translated">Jede Anzahl von Medien, die direkt um ein reguläres Textelement umhüllt wurden.</target>
      </trans-unit>
      <trans-unit id="common_imagesOnly_title">
        <source>Images Only</source>
        <target state="translated">Nur Bilder</target>
      </trans-unit>
      <trans-unit id="common_imagesOnly_description">
        <source>Any number of images aligned in columns and rows with a caption.</source>
        <target state="translated">Jede Anzahl von Bildern, die in Spalten und Zeilen mit einer Überschrift ausgerichtet sind.</target>
      </trans-unit>
      <trans-unit id="common_bulletList_title">
        <source>Bullet List</source>
        <target state="translated">Bullet-Liste</target>
      </trans-unit>
      <trans-unit id="common_bulletList_description">
        <source>A single bullet list.</source>
        <target state="translated">Eine einzige Kugel-Liste.</target>
      </trans-unit>
      <trans-unit id="common_table_title">
        <source>Table</source>
        <target state="translated">Tabelle</target>
      </trans-unit>
      <trans-unit id="common_table_description">
        <source>A simple table.</source>
        <target state="translated">Eine einfache Tabelle.</target>
      </trans-unit>
      <trans-unit id="menu">
        <source>Menu</source>
        <target state="translated">Menü</target>
      </trans-unit>
