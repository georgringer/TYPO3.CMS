<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns:t3="http://typo3.org/schemas/xliff" version="1.0">
  <file t3:id="1415814893" source-language="en" datatype="plaintext" original="messages" date="2011-10-17T20:22:34Z" product-name="linkvalidator" target-language="de">
    <header/>
    <body>
      <trans-unit id="mod_linkvalidator">
        <source>Linkvalidator</source>
        <target state="translated">Linkvalidator</target>
      </trans-unit>
      <trans-unit id="tasks.validate.name">
        <source>Linkvalidator</source>
        <target state="translated">Linkvalidator</target>
      </trans-unit>
      <trans-unit id="tasks.validate.description">
        <source>Search for broken links and store the result into the temporary table tx_linkvalidator_link in order to ease up the backend module.</source>
        <target state="translated">Suche nach kaputten Links und speichere das Ergebnis in die temporäre Tabelle tx_linkvalidator_link, um das Backend-Modul zu vereinfachen.</target>
      </trans-unit>
      <trans-unit id="tasks.validate.page">
        <source>Start page (uid)</source>
        <target state="translated">Startseite (uid)</target>
      </trans-unit>
      <trans-unit id="tasks.validate.depth">
        <source>Depth</source>
        <target state="translated">Tiefe</target>
      </trans-unit>
      <trans-unit id="tasks.validate.conf">
        <source>Overwrite TSconfig</source>
        <target state="translated">TSconfig überschreiben</target>
      </trans-unit>
      <trans-unit id="tasks.validate.email">
        <source>Send email report to</source>
        <target state="translated">E-Mail-Bericht senden an</target>
      </trans-unit>
      <trans-unit id="tasks.validate.emailOnBrokenLinkOnly">
        <source>Send email on new broken links only</source>
        <target state="translated">Nur auf neuen, gebrochenen Links E-Mail senden</target>
      </trans-unit>
      <trans-unit id="tasks.validate.emailTemplateFile">
        <source>Email template file</source>
        <target state="translated">E-Mail-Vorlagendatei</target>
      </trans-unit>
      <trans-unit id="tasks.validate.invalidEmail">
        <source>Invalid email format!</source>
        <target state="translated">Ungültiges E-Mail-Format!</target>
      </trans-unit>
      <trans-unit id="tasks.validate.invalidPage">
        <source>Invalid page uid, please enter a valid page uid!</source>
        <target state="translated">Ungültige Seite uid, bitte geben Sie eine gültige Seite uid!</target>
      </trans-unit>
      <trans-unit id="tasks.validate.invalidDepth">
        <source>There is no depth set, please set it to one of the offered values!</source>
        <target state="translated">Es gibt keine Tiefe, bitte setzen Sie sie auf einen der angebotenen Werte!</target>
      </trans-unit>
      <trans-unit id="tasks.error.noSubject">
        <source>No subject for the notification email</source>
        <target state="translated">Kein Betreff für die Benachrichtigungs-E-Mail</target>
      </trans-unit>
      <trans-unit id="tasks.error.invalidToEmail">
        <source>Invalid format of one or more of the recipient email addresses!</source>
        <target state="translated">Ungültiges Format einer oder mehrerer Empfänger-E-Mail-Adressen!</target>
      </trans-unit>
      <trans-unit id="tasks.error.invalidFromEmail">
        <source>Invalid format of the email address in the from header</source>
        <target state="translated">Ungültiges Format der E-Mail-Adresse im Header</target>
      </trans-unit>
      <trans-unit id="tasks.error.invalidTSconfig">
        <source>Invalid TSconfig in the task configuration!</source>
        <target state="translated">Ungültige TSconfig in der Task-Konfiguration!</target>
      </trans-unit>
      <trans-unit id="tasks.error.invalidEmailTemplateFile">
        <source>The email template file is not existing!</source>
        <target state="translated">Die E-Mail-Vorlage-Datei ist nicht vorhanden!</target>
      </trans-unit>
      <trans-unit id="tasks.error.invalidPageUid">
        <source>The given page id %d is invalid!</source>
        <target state="translated">Die angegebene Seite-ID %d ist ungültig!</target>
      </trans-unit>
    </body>
  </file>
