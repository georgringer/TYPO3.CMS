<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns:t3="http://typo3.org/schemas/xliff" version="1.0">
  <file t3:id="1415814993" source-language="en" datatype="plaintext" original="messages" date="2011-10-17T20:22:37Z" product-name="sys_action" target-language="de">
    <header/>
    <body>
      <trans-unit id="sys_action">
        <source>Action</source>
        <target state="translated">Aktion</target>
      </trans-unit>
      <trans-unit id="description">
        <source>An 'Action' is a configuration of a specific task which may be performed by assigned usergroups via the Task Center.</source>
        <target state="translated">Eine 'Aktion' ist eine Konfiguration einer spezifischen Aufgabe, die durch zugewiesene Benutzergruppen über das Task Center ausgeführt werden kann.</target>
      </trans-unit>
      <trans-unit id="new-sys_action">
        <source>Create a new action</source>
        <target state="translated">Neue Aktion erstellen</target>
      </trans-unit>
      <trans-unit id="edit-sys_action">
        <source>Edit record</source>
        <target state="translated">Eintrag bearbeiten</target>
      </trans-unit>
      <trans-unit id="action_error-not-found">
        <source>The action doesn't exist</source>
        <target state="translated">Die Aktion existiert nicht</target>
      </trans-unit>
      <trans-unit id="action_not-found">
        <source>No actions found</source>
        <target state="translated">Keine Aktionen gefunden</target>
      </trans-unit>
      <trans-unit id="action_not-found-description">
        <source>Currently there are no actions yet defined!</source>
        <target state="translated">Derzeit sind noch keine Aktionen definiert!</target>
      </trans-unit>
      <trans-unit id="action_error-access">
        <source>You don't have sufficient rights to see the records. Please ask the administrator to set those for you.</source>
        <target state="translated">Sie haben nicht genügend Rechte, um die Datensätze zu sehen. Bitte fragen Sie den Administrator, diese für Sie zu setzen.</target>
      </trans-unit>
      <trans-unit id="error-wrong-email">
        <source>The E-mail address is wrong.</source>
        <target state="translated">Die E-Mail-Adresse ist falsch.</target>
      </trans-unit>
      <trans-unit id="error-username-empty">
        <source>The username is empty.</source>
        <target state="translated">Der Benutzername ist leer.</target>
      </trans-unit>
      <trans-unit id="error-password-empty">
        <source>The password is empty.</source>
        <target state="translated">Das Passwort ist leer.</target>
      </trans-unit>
      <trans-unit id="error-wrong-user">
        <source>You are not allowed to edit this user.</source>
        <target state="translated">Sie sind nicht berechtigt, diesen Benutzer zu bearbeiten.</target>
      </trans-unit>
      <trans-unit id="success">
        <source>Success</source>
        <target state="translated">Erfolg</target>
      </trans-unit>
      <trans-unit id="success-user-created">
        <source>The user has been created.</source>
        <target state="translated">Der Benutzer wurde erstellt.</target>
      </trans-unit>
      <trans-unit id="success-user-updated">
        <source>The user has been updated.</source>
        <target state="translated">Der Benutzer wurde aktualisiert.</target>
      </trans-unit>
      <trans-unit id="action_noType">
        <source>Error: The action had no type defined.</source>
        <target state="translated">Fehler: Die Aktion hatte keinen Typ definiert.</target>
      </trans-unit>
      <trans-unit id="action_notReady">
        <source>Error: The action configuration is not finished.</source>
        <target state="translated">Fehler: Die Aktion-Konfiguration ist nicht beendet.</target>
      </trans-unit>
      <trans-unit id="action_download_file">
        <source>Click here to download the exported data in a file</source>
        <target state="translated">Klicken Sie hier, um die exportierten Daten in einer Datei herunterzuladen</target>
      </trans-unit>
      <trans-unit id="action_editQuery">
        <source>Edit Query</source>
        <target state="translated">Abfrage bearbeiten</target>
      </trans-unit>
      <trans-unit id="action_createQuery">
        <source>Create Query</source>
        <target state="translated">Abfrage erstellen</target>
      </trans-unit>
      <trans-unit id="action_emptyQuery">
        <source>Query is empty, create one first!</source>
        <target state="translated">Abfrage ist leer, erstelle zuerst!</target>
      </trans-unit>
      <trans-unit id="action_error">
        <source>Action error!</source>
        <target state="translated">Aktionsfehler!</target>
      </trans-unit>
      <trans-unit id="action_lowlevelMissing">
        <source>The extension 'lowlevel' must be installed in order to create a query.</source>
        <target state="translated">Die Erweiterung 'lowlevel' muss installiert werden, um eine Abfrage zu erstellen.</target>
      </trans-unit>
      <trans-unit id="action_t2_result">
        <source>Result of query</source>
        <target state="translated">Ergebnis der Abfrage</target>
      </trans-unit>
      <trans-unit id="action_Create">
        <source>Create Backend User</source>
        <target state="translated">Backend Benutzer erstellen</target>
      </trans-unit>
      <trans-unit id="action_Update">
        <source>Update Backend User</source>
        <target state="translated">Backend Benutzer aktualisieren</target>
      </trans-unit>
      <trans-unit id="action_t1_listOfUsers">
        <source>List of backend users</source>
        <target state="translated">Liste der Backend-Benutzer</target>
      </trans-unit>
      <trans-unit id="action_t1_legend_generalFields">
        <source>General fields</source>
        <target state="translated">Allgemeine Felder</target>
      </trans-unit>
      <trans-unit id="action_t1_legend_configuration">
        <source>Configuration</source>
        <target state="translated">Konfiguration</target>
      </trans-unit>
      <trans-unit id="action_toolbaritem">
        <source>Open System Actions</source>
        <target state="translated">Systemaktionen öffnen</target>
      </trans-unit>
    </body>
  </file>
</xliff>
