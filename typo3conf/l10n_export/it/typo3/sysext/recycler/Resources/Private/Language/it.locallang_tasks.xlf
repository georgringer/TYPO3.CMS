  <file t3:id="1424776442" source-language="en" datatype="plaintext" original="messages" date="2011-10-17T20:22:35Z" product-name="recycler" target-language="it">
    <header/>
    <body>
      <trans-unit id="cleanerTaskTitle">
        <source>Remove deleted records</source>
        <target state="translated">Rimuovi i record cancellati</target>
      </trans-unit>
      <trans-unit id="cleanerTaskDescription">
        <source>Deletes rows from the database which are flagged "deleted"</source>
        <target state="translated">Elimina le righe dal database che sono contrassegnate "eliminate"</target>
      </trans-unit>
      <trans-unit id="cleanerTaskPeriod">
        <source>Delete entries older than (in days)</source>
        <target state="translated">Elimina le voci più vecchie di (in giorni)</target>
      </trans-unit>
      <trans-unit id="cleanerTaskTCA">
        <source>Tables</source>
        <target state="translated">Tabelle</target>
      </trans-unit>
      <trans-unit id="cleanerTaskDescriptionTables">
        <source>Tables: %s</source>
        <target state="translated">Tabelle: %s</target>
      </trans-unit>
      <trans-unit id="cleanerTaskDescriptionDays">
        <source>Older than %d days(s)</source>
        <target state="translated">Più vecchi di %d giorni(i)</target>
      </trans-unit>
      <trans-unit id="cleanerTaskErrorPeriod">
        <source>The period has to be an integer and greater than zero.</source>
        <target state="translated">Il periodo deve essere un intero e maggiore di zero.</target>
      </trans-unit>
      <trans-unit id="cleanerTaskErrorTCAempty">
        <source>You have to select at least one table.</source>
        <target state="translated">Devi selezionare almeno una tabella.</target>
      </trans-unit>
      <trans-unit id="cleanerTaskErrorTCANotSet">
        <source>The table "%s" is not in the TCA.</source>
        <target state="translated">La tabella "%s" non è nel TCA.</target>
      </trans-unit>
    </body>
