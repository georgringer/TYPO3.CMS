<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns:t3="http://typo3.org/schemas/xliff" version="1.0">
  <file t3:id="1415814881" source-language="en" datatype="plaintext" original="messages" date="2011-10-17T20:22:34Z" product-name="lang" target-language="it">
    <header/>
    <body>
      <trans-unit id="mlang_labels_tablabel">
        <source>List of database-records</source>
        <target state="translated">Elenco dei database-record</target>
      </trans-unit>
      <trans-unit id="mlang_labels_tabdescr">
        <source>The Web&gt;List module provides low level access to records appearing on a page.</source>
        <target state="translated">Il modulo Lista&gt;Web fornisce un accesso a basso livello ai record che appaiono in una pagina.</target>
      </trans-unit>
      <trans-unit id="mlang_tabs_tab">
        <source>List</source>
        <target state="translated">Lista</target>
      </trans-unit>
      <trans-unit id="clip_pasteAfter">
        <source>Paste after: Clipboard content is inserted after this record</source>
        <target state="translated">Incolla dopo: il contenuto degli appunti è inserito dopo questo record</target>
      </trans-unit>
      <trans-unit id="clip_pasteInto">
        <source>Paste into: Clipboard content is inserted on this page</source>
        <target state="translated">Incolla in: il contenuto degli appunti è inserito in questa pagina</target>
      </trans-unit>
      <trans-unit id="clip_paste">
        <source>Paste in clipboard content</source>
        <target state="translated">Incolla nel contenuto degli appunti</target>
      </trans-unit>
      <trans-unit id="clip_markRecords">
        <source>Mark All/Mark none</source>
        <target state="translated">Segna tutti/Segna nessuno</target>
      </trans-unit>
      <trans-unit id="clip_selectMarked">
        <source>Transfer the selection of records to clipboard</source>
        <target state="translated">Trasferisci la selezione dei record negli appunti</target>
      </trans-unit>
      <trans-unit id="clip_duplicates">
        <source>Mark duplicates</source>
        <target state="translated">Segna duplicati</target>
      </trans-unit>
      <trans-unit id="clip_editMarked">
        <source>Edit marked</source>
        <target state="translated">Modifica contrassegnata</target>
      </trans-unit>
      <trans-unit id="clip_deleteMarked">
        <source>Delete marked</source>
        <target state="translated">Elimina contrassegnato</target>
      </trans-unit>
      <trans-unit id="clip_deleteMarkedWarning">
        <source>Are you sure you want to delete all marked records from the table '%s'?</source>
        <target state="translated">Sei sicuro di voler eliminare tutti i record contrassegnati dalla tabella '%s'?</target>
      </trans-unit>
      <trans-unit id="showInfo">
        <source>Display information</source>
        <target state="translated">Mostra informazioni</target>
      </trans-unit>
      <trans-unit id="edit">
        <source>Edit record</source>
        <target state="translated">Modifica record</target>
      </trans-unit>
      <trans-unit id="editPage">
        <source>Edit page properties</source>
        <target state="translated">Modifica proprietà pagina</target>
      </trans-unit>
      <trans-unit id="new">
        <source>New record</source>
        <target state="translated">Nuovo record</target>
      </trans-unit>
      <trans-unit id="newPage">
        <source>Create new page after this page</source>
        <target state="translated">Crea una nuova pagina dopo questa pagina</target>
      </trans-unit>
      <trans-unit id="newRecord">
        <source>Create new record after this record</source>
        <target state="translated">Crea un nuovo record dopo questo record</target>
      </trans-unit>
      <trans-unit id="newRecordGeneral">
        <source>Create new record</source>
        <target state="translated">Crea nuovo record</target>
      </trans-unit>
      <trans-unit id="moveUp">
        <source>Move up in list</source>
        <target state="translated">Sposta in alto nella lista</target>
      </trans-unit>
      <trans-unit id="moveDown">
        <source>Move down in list</source>
        <target state="translated">Sposta in basso nella lista</target>
      </trans-unit>
      <trans-unit id="move_record">
        <source>Re-position content element</source>
        <target state="translated">Riposizione elemento contenuto</target>
      </trans-unit>
      <trans-unit id="move_page">
        <source>Move page</source>
        <target state="translated">Sposta pagina</target>
      </trans-unit>
      <trans-unit id="hide">
        <source>Hide record</source>
        <target state="translated">Nascondi record</target>
      </trans-unit>
      <trans-unit id="unHide">
        <source>Un-hide record</source>
        <target state="translated">Nascondere record</target>
      </trans-unit>
      <trans-unit id="hidePage">
        <source>Hide page</source>
        <target state="translated">Nascondi pagina</target>
      </trans-unit>
      <trans-unit id="unHidePage">
        <source>Un-hide page</source>
        <target state="translated">Nascondi pagina</target>
      </trans-unit>
      <trans-unit id="prevLevel">
        <source>Move this page to the position after the current parent page (Inwards)</source>
        <target state="translated">Sposta questa pagina nella posizione dopo la pagina padre corrente (Inwards)</target>
      </trans-unit>
      <trans-unit id="nextLevel">
        <source>Move this page to be a subpage of the page above (Outwards)</source>
        <target state="translated">Sposta questa pagina come una pagina di pagina superiore (in uscita)</target>
      </trans-unit>
      <trans-unit id="delete">
        <source>Delete record (!)</source>
        <target state="translated">Elimina record (!)</target>
      </trans-unit>
      <trans-unit id="restore">
        <source>Restore record</source>
        <target state="translated">Ripristina record</target>
      </trans-unit>
      <trans-unit id="history">
        <source>Display change history / Un-do</source>
        <target state="translated">Mostra cronologia modifiche / Non-fare</target>
      </trans-unit>
      <trans-unit id="displayVersions">
        <source>Display versions</source>
        <target state="translated">Visualizza versioni</target>
      </trans-unit>
      <trans-unit id="permissions">
        <source>Set permissions for page</source>
        <target state="translated">Imposta i permessi per la pagina</target>
      </trans-unit>
      <trans-unit id="largeControl">
        <source>Extended view</source>
        <target state="translated">Vista estesa</target>
      </trans-unit>
      <trans-unit id="contractView">
        <source>List all tables</source>
        <target state="translated">Elenca tutte le tabelle</target>
      </trans-unit>
      <trans-unit id="expandView">
        <source>List only this table</source>
        <target state="translated">Elenca solo questa tabella</target>
      </trans-unit>
      <trans-unit id="showClipBoard">
        <source>Show clipboard</source>
        <target state="translated">Mostra appunti</target>
      </trans-unit>
      <trans-unit id="editShownColumns">
        <source>Edit all shown fields of the listed records</source>
        <target state="translated">Modifica tutti i campi mostrati dei record elencati</target>
      </trans-unit>
      <trans-unit id="editThisColumn">
        <source>Edit the '%s' field of the listed records</source>
        <target state="translated">Modifica il campo '%s' dei record elencati</target>
      </trans-unit>
      <trans-unit id="deleteWarning">
        <source>Are you sure you want to delete this record?</source>
        <target state="translated">Sei sicuro di voler eliminare questo record?</target>
      </trans-unit>
      <trans-unit id="restoreWarning">
        <source>Are you sure you want to restore this record by reverting the delete status?</source>
        <target state="translated">Sei sicuro di voler ripristinare questo record ripristinando lo stato di eliminazione?</target>
      </trans-unit>
      <trans-unit id="localization">
        <source>Localization view</source>
        <target state="translated">Vista localizzazione</target>
      </trans-unit>
      <trans-unit id="defaultLanguage">
        <source>Default</source>
        <target state="translated">Predefinito</target>
      </trans-unit>
      <trans-unit id="multipleLanguages">
        <source>All languages</source>
        <target state="translated">Tutte le lingue</target>
      </trans-unit>
      <trans-unit id="Localize">
        <source>Localize to</source>
        <target state="translated">Localizza in</target>
      </trans-unit>
      <trans-unit id="viewRecords">
        <source>Viewing records of</source>
        <target state="translated">Visualizzazione record di</target>
      </trans-unit>
      <trans-unit id="pageIndicator">
        <source>Page %1$s of %2$d</source>
        <target state="translated">Pagina %1$s di %2$d</target>
      </trans-unit>
      <trans-unit id="rangeIndicator">
        <source>Records %1$d - %2$d</source>
        <target state="translated">Record %1$d - %2$d</target>
      </trans-unit>
      <trans-unit id="missingTcaColumnsMessageTitle">
        <source>Missing TCA definition</source>
        <target state="translated">Definizione TCA mancante</target>
      </trans-unit>
      <trans-unit id="missingTcaColumnsMessage">
        <source>No fields for table "%s" have been found. $TCA['%s']['columns'] does not contain the necessary definitions.</source>
        <target state="translated">Nessun campo per la tabella "%s" è stato trovato. $TCA['%s']['columns'] non contiene le definizioni necessarie.</target>
      </trans-unit>
      <trans-unit id="noRecordsOnThisPage">
        <source>There are no records on this page.</source>
        <target state="translated">Non ci sono record in questa pagina.</target>
      </trans-unit>
      <trans-unit id="button.cancel">
        <source>Cancel</source>
        <target state="translated">Annulla</target>
      </trans-unit>
      <trans-unit id="button.close">
        <source>Close</source>
        <target state="translated">Chiudi</target>
      </trans-unit>
      <trans-unit id="button.ok">
        <source>OK</source>
        <target state="translated">OK</target>
      </trans-unit>
      <trans-unit id="button.delete">
        <source>Delete</source>
        <target state="translated">Elimina</target>
      </trans-unit>
    </body>
  </file>
