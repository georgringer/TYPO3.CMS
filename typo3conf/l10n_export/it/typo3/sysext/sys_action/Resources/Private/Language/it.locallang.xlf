<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns:t3="http://typo3.org/schemas/xliff" version="1.0">
  <file t3:id="1415814993" source-language="en" datatype="plaintext" original="messages" date="2011-10-17T20:22:37Z" product-name="sys_action" target-language="it">
    <header/>
    <body>
      <trans-unit id="sys_action">
        <source>Action</source>
        <target state="translated">Azione</target>
      </trans-unit>
      <trans-unit id="description">
        <source>An 'Action' is a configuration of a specific task which may be performed by assigned usergroups via the Task Center.</source>
        <target state="translated">Un'azione è una configurazione di un'attività specifica che può essere eseguita da gruppi di utenti assegnati tramite il Centro Task.</target>
      </trans-unit>
      <trans-unit id="new-sys_action">
        <source>Create a new action</source>
        <target state="translated">Crea una nuova azione</target>
      </trans-unit>
      <trans-unit id="edit-sys_action">
        <source>Edit record</source>
        <target state="translated">Modifica record</target>
      </trans-unit>
      <trans-unit id="action_error-not-found">
        <source>The action doesn't exist</source>
        <target state="translated">L'azione non esiste</target>
      </trans-unit>
      <trans-unit id="action_not-found">
        <source>No actions found</source>
        <target state="translated">Nessuna azione trovata</target>
      </trans-unit>
      <trans-unit id="action_not-found-description">
        <source>Currently there are no actions yet defined!</source>
        <target state="translated">Al momento non ci sono azioni ancora definite!</target>
      </trans-unit>
      <trans-unit id="action_error-access">
        <source>You don't have sufficient rights to see the records. Please ask the administrator to set those for you.</source>
        <target state="translated">Non hai i permessi sufficienti per vedere i record. Si prega di chiedere all'amministratore di impostarli per te.</target>
      </trans-unit>
      <trans-unit id="error-wrong-email">
        <source>The E-mail address is wrong.</source>
        <target state="translated">L'indirizzo e-mail è sbagliato.</target>
      </trans-unit>
      <trans-unit id="error-username-empty">
        <source>The username is empty.</source>
        <target state="translated">Il nome utente è vuoto.</target>
      </trans-unit>
      <trans-unit id="error-password-empty">
        <source>The password is empty.</source>
        <target state="translated">La password è vuota.</target>
      </trans-unit>
      <trans-unit id="error-wrong-user">
        <source>You are not allowed to edit this user.</source>
        <target state="translated">Non sei autorizzato a modificare questo utente.</target>
      </trans-unit>
      <trans-unit id="success">
        <source>Success</source>
        <target state="translated">Successo</target>
      </trans-unit>
      <trans-unit id="success-user-created">
        <source>The user has been created.</source>
        <target state="translated">L'utente è stato creato.</target>
      </trans-unit>
      <trans-unit id="success-user-updated">
        <source>The user has been updated.</source>
        <target state="translated">L'utente è stato aggiornato.</target>
      </trans-unit>
      <trans-unit id="action_noType">
        <source>Error: The action had no type defined.</source>
        <target state="translated">Errore: L'azione non ha definito alcun tipo.</target>
      </trans-unit>
      <trans-unit id="action_notReady">
        <source>Error: The action configuration is not finished.</source>
        <target state="translated">Errore: La configurazione dell'azione non è terminata.</target>
      </trans-unit>
      <trans-unit id="action_download_file">
        <source>Click here to download the exported data in a file</source>
        <target state="translated">Clicca qui per scaricare i dati esportati in un file</target>
      </trans-unit>
      <trans-unit id="action_editQuery">
        <source>Edit Query</source>
        <target state="translated">Modifica Query</target>
      </trans-unit>
      <trans-unit id="action_createQuery">
        <source>Create Query</source>
        <target state="translated">Crea Query</target>
      </trans-unit>
      <trans-unit id="action_emptyQuery">
        <source>Query is empty, create one first!</source>
        <target state="translated">La query è vuota, crearne una prima!</target>
      </trans-unit>
      <trans-unit id="action_error">
        <source>Action error!</source>
        <target state="translated">Errore di azione!</target>
      </trans-unit>
      <trans-unit id="action_lowlevelMissing">
        <source>The extension 'lowlevel' must be installed in order to create a query.</source>
        <target state="translated">L'estensione 'lowlevel' deve essere installata per creare una query.</target>
      </trans-unit>
      <trans-unit id="action_t2_result">
        <source>Result of query</source>
        <target state="translated">Risultato della query</target>
      </trans-unit>
      <trans-unit id="action_Create">
        <source>Create Backend User</source>
        <target state="translated">Crea Utente Backend</target>
      </trans-unit>
      <trans-unit id="action_Update">
        <source>Update Backend User</source>
        <target state="translated">Aggiorna Utente Backend</target>
      </trans-unit>
      <trans-unit id="action_t1_listOfUsers">
        <source>List of backend users</source>
        <target state="translated">Elenco degli utenti backend</target>
      </trans-unit>
      <trans-unit id="action_t1_legend_generalFields">
        <source>General fields</source>
        <target state="translated">Campi generali</target>
      </trans-unit>
      <trans-unit id="action_t1_legend_configuration">
        <source>Configuration</source>
        <target state="translated">Configurazione</target>
      </trans-unit>
      <trans-unit id="action_toolbaritem">
        <source>Open System Actions</source>
        <target state="translated">Azioni di sistema aperte</target>
      </trans-unit>
    </body>
  </file>
</xliff>
